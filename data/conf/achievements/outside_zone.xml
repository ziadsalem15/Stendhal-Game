	<achievement name="junior explorer">
	  	<type class="achievements" subclass="outside_zone" tileid="-1"/>
		<implementation class-name="games.stendhal.server.entity.core.rp.achievement.factory.OutsideZoneAchievementFactory"/>
		<attributes>
			<identifier value="zone.outside.semos" />
			<title value="Junior Explorer" />
			<description value="Visit all outside zones in the Semos region"/>
			<score value="1"/>
			<active value="true"/>
			<condition value="new PlayerVisitedZonesInRegionCondition('semos', Boolean.TRUE, Boolean.TRUE)"/>
		</attributes>
	</achievement>

