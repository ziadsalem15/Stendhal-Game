	<achievement name="semostest">
		<type class="achievements" subclass="interior_zone.png" tileid="-1"/>
		<implementation class-name="games.stendhal.server.InteriorZoneAchievementFactory"/>
		<attributes>
			<identifier value = "zone.interior.semos" />
			<title value = "Home maker" />
			<description value = "Visit all interior zones in the Semos region" />
			<score value = "2" />
			<active value = "true" />
			<condition value = "new PlayerVisitedZonesInRegionCondition('semos', Boolean.FALSE, Boolean.FALSE)" />
		</attributes>
	</achievement>
	
